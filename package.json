{
  "name": "@vollowx/see",
  "version": "0.1.0",
  "description": "Standard Extensible Elements. A web components library that can be styled and extended freely, while pre-providing components in Material 3 design guideline.",
  "author": "vollowx",
  "license": "MIT",
  "type": "module",
  "main": "./all.js",
  "module": "./all.js",
  "exports": {
    ".": "./all.js",
    "./m3/*": "./src/m3/*.js",
    "./base/*": "./src/base/*.js"
  },
  "files": [
    "src/**/*.js",
    "src/**/*.d.ts",
    "all.js",
    "all.d.ts",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "web-components",
    "material-design",
    "material-3",
    "lit",
    "ui-components"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vollowx/see.git"
  },
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "wds --watch --node-resolve",
    "build": "node scripts/convert-styles.mjs build && tsc",
    "cleanup": "node scripts/cleanup.mjs",
    "prepublishOnly": "bun cleanup && bun run build",
    "styles": "node scripts/convert-styles.mjs",
    "styles:build": "node scripts/convert-styles.mjs build",
    "styles:watch": "node scripts/convert-styles.mjs watch",
    "styles:sort": "bunx postcss \"src/**/*.css\" --replace --no-map",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --check --write",
    "docs:dev": "vite --open /docs/ --host",
    "docs:build": "node scripts/convert-styles.mjs build && vite build docs",
    "docs:preview": "vite preview docs --host"
  },
  "devDependencies": {
    "@web/dev-server": "^0.4.6",
    "@web/dev-server-esbuild": "^1.0.2",
    "chokidar": "^5.0.0",
    "lightningcss": "^1.25.1",
    "postcss": "^8.4.49",
    "postcss-cli": "^11.0.0",
    "postcss-sorting": "^9.0.0",
    "prettier": "^3.3.2",
    "rollup": "^4.18.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^5.5.3",
    "vite": "^5.3.3",
    "vite-plugin-html": "^3.2.2"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.6.7",
    "lit": "^3.1.4",
    "tslib": "^2.6.3"
  }
}
