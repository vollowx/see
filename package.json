{
  "name": "see",
  "version": "0.0.0",
  "description": "Standard Extensible Elements",
  "author": "vollow",
  "license": "MIT",
  "type": "module",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "node scripts/convert-styles.mjs build && tsc",
    "dev": "wds --watch --node-resolve",
    "docs:dev": "vite --open /docs/ --host",
    "docs:build": "node scripts/convert-styles.mjs build && vite build docs",
    "docs:preview": "vite preview docs --host",
    "styles": "node scripts/convert-styles.mjs",
    "styles:build": "node scripts/convert-styles.mjs build",
    "styles:watch": "node scripts/convert-styles.mjs watch",
    "cleanup": "node scripts/cleanup.mjs",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --check --write"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@web/dev-server": "^0.4.6",
    "@web/dev-server-esbuild": "^1.0.2",
    "lightningcss": "^1.25.1",
    "prettier": "^3.3.2",
    "rollup": "^4.18.1",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3",
    "vite": "^5.3.3",
    "vite-plugin-html": "^3.2.2"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.6.7",
    "chokidar": "^5.0.0",
    "lit": "^3.1.4"
  }
}
