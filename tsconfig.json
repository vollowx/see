{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "baseUrl": ".",
    "importHelpers": true,
    "moduleResolution": "Node",
    "noEmit": true,
    "noImplicitAny": false,
    // "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    // "noUnusedLocals": true,
    "strict": true,
    "strictNullChecks": false,
    "target": "ESNext"
  },
  "exclude": []
}
